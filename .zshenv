export  _JAVA_AWT_WM_NONREPARENTING=1
export EDITOR=nvim
export TERMINAL=alacritty
export BROWSER=brave
export FILEMAN=pcmanfm

alias dotfiles="git --git-dir=$HOME/.dotfiles --work-tree=$HOME"

alias die="sudo poweroff"
alias rbt="sudo reboot"
alias c="clear"

alias nf="neofetch"
alias maims="maim --select"
alias v="$EDITOR"
alias vim="$EDITOR"
alias s="sudo"
alias mi="make install"
alias ll="lsd -lA --color"
alias ls="lsd"
alias mf="mkf"
alias md="mkdir"
alias fm="ranger"

alias g="git"
alias gs="git status"
alias gcm="git commit -a -m"
alias ga="git add"
alias gpl="git pull"
alias gp="git push"
alias gi="git init"
alias gc="git clone"
alias gd="git diff"
